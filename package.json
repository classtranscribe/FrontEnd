{
  "name": "class-transcribe-frontend",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/classtranscribe/FrontEnd.git"
  },
  "scripts": {
    "start": "node write_env.js ./public/config.js && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "lint": "cross-env NODE_ENV=production eslint .",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --list-different \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "build:staging": "docker build -t classtranscribe/frontend:staging .",
    "build:prod": "docker build -t classtranscribe/frontend:latest .",
    "push:staging": "docker push classtranscribe/frontend:staging",
    "push:prod": "docker push classtranscribe/frontend",
    "deploy:staging": "yarn build:staging && yarn push:staging",
    "deploy:prod": "yarn build:prod && yarn push:prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "yarn lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@microsoft/applicationinsights-react-js": "^2.5.3",
    "@microsoft/applicationinsights-web": "^2.5.3",
    "acorn": "^6.4.1",
    "adm-zip": "^0.4.14",
    "auth0-js": "^9.13.2",
    "autosize": "^4.0.2",
    "axios": "^0.19.0",
    "babel": "^6.23.0",
    "bootstrap": "^4.4.1",
    "braft-editor": "^2.3.9",
    "classnames": "^2.2.6",
    "dentist": "^1.0.3",
    "highlight.js": "^10.0.1",
    "jquery": "^3.5.1",
    "js-file-download": "^0.4.9",
    "jwt-decode": "^2.2.0",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.14",
    "mdx.macro": "^0.2.9",
    "mk-guide": "^1.10.5",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "npm": "^6.13.4",
    "original-fs": "^1.1.0",
    "papaparse": "^5.1.1",
    "pico-ui": "^0.0.6",
    "popper.js": "^1.16.1",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-ace": "^8.1.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dates": "^20.3.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.1.5",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-vis": "^1.11.7",
    "react-with-direction": "^1.3.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.3",
    "showdown": "^1.9.1",
    "showdown-katex": "^0.8.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-env": "^7.0.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
